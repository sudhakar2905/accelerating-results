{
  "manifest_version": 3,
  "name": "Accelerating Results",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"],
      "css":["styles.css"]
    }
  ]
}
